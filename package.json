    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "mongodb": "^6.18.0",
    "morgan": "~1.9.1",
    "pg": "^8.16.3",
    "pug": "2.0.0-beta11"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "description": "Backend REST API for WebGIS applications providing feature-linked tabular data",
  "main": "app.js",
  "repository": {
    "type": "git",
